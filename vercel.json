{
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "^/pages/(.*)",
      "dest": "/pages/$1.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "src": "/src/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}